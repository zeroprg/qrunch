.login-form {
	background-image: url(../images/login-bg.png);
	background-repeat: no-repeat;
	background-attachment: fixed;
	background-size: 100% 100%;
	background-position: top left;
}

.login-btn {
	background-color: #3a8fc7;
	border: 0;
	color: #fff;
	text-shadow: 0 1px 1px #222;
	padding-top: 10px;
	padding-bottom: 10px;
}

.login-btn-pressed {
	background-color: #5ae;
}

.logo {
	background-position: top center;
}

.email-field-label {
	background-image: url(../images/email-field-label.png);
	background-repeat: no-repeat;
	background-size: 40px 100%;
	background-color: transparent;
}

.password-field-label {
	background-image: url(../images/password-field-label.png);
	background-repeat: no-repeat;
	background-size: 40px 100%;
	background-color: transparent;
}

.bg {
/*	background-image: url(../images/login-bg.png);
	background-repeat: no-repeat;
	background-attachment: fixed;
	background-size: 100% 100%;
	background-position: top left;*/
	background-image: linear-gradient(bottom, rgb(16,18,21) 0%, rgb(27,39,53) 50%, rgb(16,18,21) 100%);
	background-image: -o-linear-gradient(bottom, rgb(16,18,21) 0%, rgb(27,39,53) 50%, rgb(16,18,21) 100%);
	background-image: -moz-linear-gradient(bottom, rgb(16,18,21) 0%, rgb(27,39,53) 50%, rgb(16,18,21) 100%);
	background-image: -webkit-linear-gradient(bottom, rgb(16,18,21) 0%, rgb(27,39,53) 50%, rgb(16,18,21) 100%);
	background-image: -ms-linear-gradient(bottom, rgb(16,18,21) 0%, rgb(27,39,53) 50%, rgb(16,18,21) 100%);
	background-image: -webkit-gradient(
		linear,
		left bottom,
		left top,
		color-stop(0, rgb(16,18,21)),
		color-stop(0.5, rgb(27,39,53)),
		color-stop(1, rgb(16,18,21))
	);
}

.top-bar {
	background-color: #212121;
	/*background-image: repeating-linear-gradient(-45deg, transparent, transparent 2px, #2e2e2e 1px, #2e2e2e 3px);*/
	background-image: url(data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAUAAAAFCAIAAAACDbGyAAAAGXRFWHRTb2Z0d2FyZQBBZG9iZSBJbWFnZVJlYWR5ccllPAAAAEZJREFUeNoUysENADEIA0GSwANSCP03FySQ4HzP9ZjdvapUlZm7e2emmSFm5r3HkHMOJCLWWltEIAgi+o8Q3CD3XkyfAAMAjechpOCyk60AAAAASUVORK5CYII=);
	background-repeat: repeat;
	height: 55px;
	padding: 0;
}

.top-bar-left-btn {
	background-image: url(../images/top-bar-left-btn.png);
	background-repeat: no-repeat;
	background-position: center center;
	min-width: 75px;
	min-height: 47px;
	height: 100%;
}

.search-btn {
	background-image: url(../images/search-btn.png);
	background-repeat: no-repeat;
	background-position: center center;
	min-width: 50px;
	min-height: 47px;
	height: 100%;
}

.highlight-pressed {
	background-color: rgba(255, 255, 255, 0.1) !important;
}

.centered-lbl {
	color: #fff;
	background-color: transparent;
	text-align: center;
	padding-top: 10px;
	padding-bottom: 10px;
	font-size: 20px;
}

.left-lbl {
	color: #fff;
	background-color: transparent;
	padding-top: 10px;
	padding-bottom: 10px;
	padding-left: 5%;
	font-size: 20px;
	font-weight: bold;
}

.overlay-txt {
	background-color: rgba(255, 255, 255, 0.3);
	width: 85%;
	font-size: 19px;
}

.overlay-txt input {
	color: #fff;
}

.plus-btn {
	background-color: rgba(255, 255, 255, 0.3);
	background-image: url(../images/plus-btn.png);
	background-repeat: no-repeat;
	background-position: center center;
	min-width: 50px;
	height: 100%;
	width: 15%;
}

.left-content-box, .right-content-box {
	background-color: rgba(255, 255, 255, 0.6);
	margin-bottom: 1px;
}

.left-content-box {
	margin-right: 1px;
	display: inline-block !important;
}

.interest-list {
	margin: auto 5%;
	background-color: transparent;
}

.interest-item {
	font-size: 17px;
}

.interest-item:nth-child(odd) {
	background-color: #fff;
}

.interest-item:nth-child(even) {
	background-color: #fafafa;
}

.interest-pop-down {
	position: absolute;
	top: 15%;
	right: 5px;
	float: right;
	padding: 2px;
	text-align: center;
}

.interest-pop-down button {
	background-color: #0a7202;
	padding: 4px 8px;
	border: 0;
	color: #fff;
	font-size: 13px;
}

.interest-item .interest-pop-down {
	visibility: hidden;
}

.actived-interest .interest-pop-down {
	visibility: visible;
}

.interest-txt {
	width: 75%;
}

.interest-txt input {
	color: #178c00;
	font-size: 16px;
}

.interest-plus-btn {
	background-color: #178c00;
	background-image: url(../images/plus-btn.png);
	background-repeat: no-repeat;
	background-size: 16px 16px;
	background-position: center center;
	min-width: 50px;
	height: 100%;
	width: 25%;
	border: 6px solid #fff;
}

.interest-plus-btn-pressed {
	background-color: #1a0;
}

.slider-menu {
	background-color: #021020;
}

.follow-btn{
  font-size: 14px;
  background: rgb(70,70,70);
  float: right;
  color: rgb(200,200,200);
  border-radius: 0px !important;
  border: 0px;
  font-family: "Arial Narrow",Arial, sans-serif;
}

.follow-btn-pressed{
  font-size: 14px;
  background: rgb(50,50,50);
  float:left;
  border-bottom: 3px solid rgb(150,200,0);
  color: rgb(150,200,0);
  border: 0px 0px 3px 0px;
  border-radius: 0px !important;
  font-family: "Arial Narrow",Arial, sans-serif;
}

.personal-stream-btn{
  padding: 5px;
  font-size: 14px;
  background: rgb(70,70,70);
  float: right;
  color: rgb(200,200,200);
  border-radius: 0px !important;
  border: 0px;
  font-family: "Arial Narrow",Arial, sans-serif;
}

.personal-stream-btn-pressed{
  padding: 5px;
  font-size: 14px;
  background: rgb(50,50,50);
  float:left;
  border-bottom: 3px solid rgb(150,200,0);
  color: rgb(150,200,0);
  border: 0px 0px 3px 0px;
  border-radius: 0px !important;
  font-family: "Arial Narrow",Arial, sans-serif;
}


.list-item-content{

}

div.avatar{
	width: 25%;
	float: left;
}

img.avatar{
	width: 55px;
	border-radius: 3px;
}

.user-info{
	width: 70%;
	padding: 0;
	display: inline-block;
	font-family: "Helvetica Narrow","Arial Narrow",Tahoma,Arial,Helvetica,sans-serif;
}

.x-list-disclosure{
	-webkit-mask: 0 0 url(data:image/png;base64,) no-repeat !important;
	-webkit-mask-size: 1.5em !important;
	background: gray !important;
	margin-top: 27px !important;
}

div.follow-list{
	border-top: none !important;
	border-bottom: none !important; 
}

.slider-btn{
	background: #021020; /* Old browsers */
	background: -moz-linear-gradient(top,  #021020 0%, #141e2c 100%); /* FF3.6+ */
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#021020), color-stop(100%,#141e2c)); /* Chrome,Safari4+ */
	background: -webkit-linear-gradient(top,  #021020 0%,#141e2c 100%); /* Chrome10+,Safari5.1+ */
	background: -o-linear-gradient(top,  #021020 0%,#141e2c 100%); /* Opera 11.10+ */
	background: -ms-linear-gradient(top,  #021020 0%,#141e2c 100%); /* IE10+ */
	background: linear-gradient(to bottom,  #021020 0%,#141e2c 100%); /* W3C */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#021020', endColorstr='#141e2c',GradientType=0 ); /* IE6-9 */

	border-right: 1px solid rgb(23,33,46);
	font-family: "Helvetica","Arial Narrow",Tahoma,Arial,Helvetica,sans-serif;
	color: white;
}

.slider-interest-btn{
	font-family: "Helvetica","Arial Narrow",Tahoma,Arial,Helvetica,sans-serif !important;
	color: white;
	padding-top: 27px;
}

.slider-percentage-btn{
	font-family:  "Times", "Chasm", "Calibri", "Helvetica","Arial Narrow",Tahoma,Arial,Helvetica,sans-serif !important;
	font-weight: bold;
	color: white;
	font-size: 30px;
	padding-top: 22px;
}

.slider-followers-btn{
	color: white;
	font-size: 14px;
	font-family:  "Helvetica","Arial Narrow",Tahoma,Arial,Helvetica,sans-serif !important;
	padding-top: 10px;
}

.slider-following-btn{
	background: rgb(235,235,235) !important;
	color: rgb(23,33,46);
	font-size: 14px;
	font-family:  "Helvetica","Arial Narrow",Tahoma,Arial,Helvetica,sans-serif !important;
	padding-top: 10px;
}

.follow-label{
	font-size: 15px;
	font-weight: bold;
	font-family:  "Arial Narrow",Tahoma,Arial,Helvetica,sans-serif !important;
}

.follow-number{
	margin-top: 15px;
	font-size: 28px;
}